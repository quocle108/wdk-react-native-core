  "author": "Tetherto",
  "license": "Apache-2.0",
  "dependencies": {
    "@tetherto/wdk-react-native-secure-storage": "github:tetherto/wdk-react-native-secure-storage#5ca4bc1e9e7b93947f1d5e82ed1e38483d114092",
    "@tanstack/react-query": "5.90.17",
    "expo-crypto": "15.0.8",
    "immer": "11.1.3",
    "@tetherto/pear-wrk-wdk": "github:tetherto/pear-wrk-wdk#21a76716f3c26b64725f7c1b84d95ea9ec3d9966",
    "react-native-bare-kit": "0.11.5",
    "react-native-mmkv": "4.1.1",
    "zod": "3.25.76",
    "zustand": "5.0.10"
  },
  "devDependencies": {
    "@testing-library/jest-native": "5.4.3",
    "@testing-library/react-native": "12.9.0",
    "@types/jest": "29.5.14",
    "@types/react": "19.2.8",
    "@types/react-native": "0.73.0",
    "jest": "29.7.0",
    "jest-environment-node": "29.7.0",
